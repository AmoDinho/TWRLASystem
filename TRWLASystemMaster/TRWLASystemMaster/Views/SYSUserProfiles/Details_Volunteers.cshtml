@model TRWLASystemMaster.Models.DB.SYSUserProfile

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/VolunteerLoggedInPage.cshtml";
}

<div class="jumbotron" style="text-align:left; margin-bottom:100px;">
    <h1>@Html.DisplayFor(model => model.FirstName)'s @ViewBag.Title</h1>
    <div class="quotehome"></div>
    <div class="quote1home"></div>
    <div class="quote2home"></div>
</div>



<div>

    <hr />



    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FirstName)
        </dd>

        <dt>
            @Html.Label("Surname")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LastName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            @Html.Label("Date of Birth")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DoB)
        </dd>

        <dt>
            @Html.Label("Phone number")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Phonenumber)
        </dd>



    </dl>

</div>

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <button type="button" class="btn" onclick="location.href='@Url.Action("Volunteers", "SYSUserProfiles")'">Back</button>


    </div>
</div>